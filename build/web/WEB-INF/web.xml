<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Binovi</servlet-name>
        <servlet-class>Binovi.Binovi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servleti.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PromenaLozinke</servlet-name>
        <servlet-class>Servleti.PromenaLozinke</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registracija</servlet-name>
        <servlet-class>Servleti.Registracija</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prijavapromena</servlet-name>
        <servlet-class>Servleti.Prijavapromena</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Odjava</servlet-name>
        <servlet-class>Servleti.Odjava</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pretragaosnovneLab</servlet-name>
        <servlet-class>Servleti.pretragaosnovneLab</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dodajlab</servlet-name>
        <servlet-class>Servleti.dodajlab</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dodajpro</servlet-name>
        <servlet-class>Servleti.dodajpro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Binovi</servlet-name>
        <url-pattern>/Binovi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PromenaLozinke</servlet-name>
        <url-pattern>/PromenaLozinke</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registracija</servlet-name>
        <url-pattern>/Registracija</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prijavapromena</servlet-name>
        <url-pattern>/prijavapromena</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Odjava</servlet-name>
        <url-pattern>/Odjava</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pretragaosnovneLab</servlet-name>
        <url-pattern>/pretragaosnovneLab</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dodajlab</servlet-name>
        <url-pattern>/dodajlab</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dodajpro</servlet-name>
        <url-pattern>/dodajpro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
